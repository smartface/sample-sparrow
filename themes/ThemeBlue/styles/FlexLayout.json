{
	".flexLayout": {
		"height": 200,
		"width": 200,
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "#FFFFFF",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"font": {},
		"flexProps": {
			"alignSelf": "AUTO",
			"direction": "INHERIT",
			"alignContent": "STRETCH",
			"alignItems": "STRETCH",
			"justifyContent": "FLEX_START",
			"flexWrap": "NOWRAP",
			"flexDirection": "COLUMN",
			"positionType": "RELATIVE"
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		},
		"&-smallThumb": {
			"borderWidth": 1,
			"borderRadius" : 5,
			"backgroundColor": "rgb(1,1,1,0)",
			".active": {
					"borderColor": "#FF9900"
			 },
			 ".inactive": {
					"borderColor": "rgba(0,0,0,0.1)"
			 }
		}
	},
	".sliderDrawer_layout": {
				"backgroundColor": { 
				"startColor": "#9D1B55",
				"endColor": "#D9595B",
				"direction": "DIAGONAL_LEFT"
			}
		}
}
